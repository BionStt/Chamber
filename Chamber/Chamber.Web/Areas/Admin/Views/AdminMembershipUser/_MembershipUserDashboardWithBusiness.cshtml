@model Chamber.Web.Areas.Admin.ViewModels.MembershipUserBusinessViewModel


<div class="col-md-12">
    <p><i class="fa fa-dashboard"></i> <span style="font-weight: 800;">Registered Users Dashboard</span></p>
    <div style="border:1px solid #3b5998"></div>
</div>
<div class="col-md-12">
    <br />
    <table class="table table-bordered table-striped">
        <tbody>
            <tr>
                <td>
                    <strong>Total Registered Users</strong>
                </td>
                <td>
                    @if (Model.NonPagedMembershipUsers != null)
                    {
                        <strong>@Model.NonPagedMembershipUsers.Count()</strong>
                    }
                    else
                    {
                        <strong>0</strong>
                    }
                </td>
            </tr>
            <tr>
                <td>
                    <strong>Regular Users</strong>
                </td>
                <td>
                    <strong></strong>
                </td>
            </tr>
            <tr>
                <td>
                    <strong>Chamber Members</strong>
                </td>
                <td>
                    <strong></strong>
                </td>
            </tr>
            <tr>
                <td>
                    <strong>Administrators</strong>
                </td>
                <td>
                    <strong></strong>
                </td>
            </tr>
            <tr>
                <td>
                    <strong>Active Users</strong>
                </td>
                <td>
                    <strong></strong>
                </td>
            </tr>
        </tbody>
    </table>
</div>